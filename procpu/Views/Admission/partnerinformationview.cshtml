@model procpu.Models.PartViewModel
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
</head>

<body>
    <div class="col-lg-12">
        <table id="example" class="table table-striped table-bordered" style="width:100%">
            <thead>

                <tr>
                    <th><b>Sl. No.</b></th>
                    <th><b>Partner ID</b></th>
                    <th><b>Partner Name</b></th>
                    <th><b>Partner Email ID</b></th>
                    <th><b>Partner Contact Number</b></th>
                    <th><b>Partner Country</b></th>
                    <th><b>Upload By</b></th>
                    <th><b>Upload On</b></th>
                    <th><b>MoU Upload</b></th>
                    <th><b>Action</b></th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in ViewBag.partnerview)
                {
                    <tr>
                        <td>@(ViewBag.partnerview.IndexOf(item) + 1)</td>
                        <td>@(item.partnerid)</td>
                        <td>@item.partnername</td>
                        <td>@(item.partneremail)</td>
                        <td>@item.partnercnumber</td>
                        <td>@(item.partnercountry)</td>
                        <td>@item.rco</td>
                        <td>@item.rcm.ToString("dd-MM-yyyy")</td>
                        <td><a href="@item.partnermouupld" target="_blank">View</a></td>
                        <td><a class="btn btn-danger" onclick="DeletePartner('@item.transid')"><i class="fa fa-trash"></i></a></td>
                    </tr>
                }


            </tbody>
        </table>
    </div>
    <script>
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</body>
</html>