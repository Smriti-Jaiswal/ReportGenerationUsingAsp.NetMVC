@model procpu.Models.GenViewModel
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
</head>
<body>
    <div class="col-lg-12">
        <table id="example" class="table table-striped table-bordered" style="width:100%">
            <thead>

                <tr>
                    <th><b>Sl. No.</b></th>
                    <th><b>Subject</b></th>
                    <th><b>Description</b></th>
                    <th><b>Upload By</b></th>
                    <th><b>Upload On</b></th>
                    <th><b>File Upload</b></th>
                    <th><b>Action</b></th>
                </tr>
            </thead>
            <tbody>

                @foreach (var item in ViewBag.genview)
                {
                    <tr>
                        <td>@(ViewBag.genview.IndexOf(item)+1)</td>
                        <td>@(item.cpugeninfo)</td>
                        <td>@item.cpugeninfodesc</td>
                        <td>@item.rco</td>
                        <td>@item.rcm.ToString("dd-MM-yyyy")</td>
                        <td><a href="@item.geninfoupld" target="_blank">View</a></td>
                        <td><a class="btn btn-danger" onclick="DeleteRecord('@item.transid')"><i class="fa fa-trash"></i></a></td>
                    </tr>
                }


            </tbody>
        </table>
    </div>
    <script>
        $(document).ready(function () {
            $('#example').DataTable();
        });
    </script>
</body>
</html>